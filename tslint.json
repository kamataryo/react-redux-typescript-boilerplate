{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "jsx-curly-spacing": [true, "always"],
      "max-line-length": false,
      "ordered-imports": false,
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "semicolon": [true, "never"],
      "member-ordering": false
    },
    "rulesDirectory": []
}
